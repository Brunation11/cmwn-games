<style type="text/css">
    .play {
        position: absolute;
    }
</style>

<script type="text/javascript">
    $(document).ready(function () {
        try {
            slide2.play();
        }
        catch (err) {
            //no sound - log error
        }

    $('.play').click(function () {
        $('.play').delay(1000).delay(1000, function () {
            document.location.href = "@Url.Content("~/ActionItem/AI2")";
        });
        click.play();
