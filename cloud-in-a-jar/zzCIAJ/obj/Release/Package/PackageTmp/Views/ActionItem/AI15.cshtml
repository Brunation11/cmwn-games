
@{
    ViewBag.Title = "AI15";
}





<style>
    #title img {
        margin-top: 66px;
        margin-left: 92px;
        margin-right: 0px;
        position: absolute;
        z-index: 1;
    }


    #sign img {
        float: right;
        margin-top: 384px;
        margin-left: 466px;
        position: absolute;
    }



    .flip-off img {
        position: absolute;
        -webkit-transition: opacity 0s ease-in-out;
        -moz-transition: opacity 0s ease-in-out;
        -o-transition: opacity 0s ease-in-out;
        transition: opacity 0s ease-in-out;
    }

        .flip-off img:hover {
            position: absolute;
            opacity: 0;
        }


    p, .p {
        font-family: Source Sans Pro Semibold;
        font-size: 30px;
        position: absolute;
        z-index: 2;
        width: 614px;
        text-align: center;
        margin-top: 124px;
        margin-left: 6px;
        letter-spacing: -1px;
    }


    h2, .h2 {
        font-family: Source Sans Pro;
        font-weight:600;
        font-size: 28px;
        position: absolute;
        text-align: center;
        z-index: 2;
        margin-top: 198px;
        margin-left: 101px;
    }

    b, strong {
        font-weight: 900;
        font-family: Source Sans Pro;
    }
</style>

<script type="text/javascript">
    $(document).ready(function () {
        try {
            load_15.load();
            load_15.play();
        }
        catch (err) {
            //no sound - log error
        }

        $('#btn1').hide();


        $('#btn').click(function () {
            $('#btn1').show();
    
    });

        $('#btn1').click(function () {
            $('#btn1').delay(1000).delay(1000, function () {
                document.location.href = "@Url.Content("~/ActionItem/AI16")";
            });
            try {
                btn_click.play();
            }
            catch (err) {
                //no sound - log error
            }
        });

    })
</script>




