                    </div><!--end background-intro-->

                    <div style="position:absolute; height:97.5%; width:100%; overflow:hidden;">
                        <div id="butterfly" style="position:absolute; z-index:9;">
                            <img src="~/Content/images/butterfly.gif" style="width:230px;" />
                        </div>

                        <div id="yellow" class="animate-flower2" style="position:absolute; z-index:9; margin-left:-2px; margin-top:324px;">
                            <img src="~/Content/images/flower_yellow.png" style="width:140px;" />
                        </div>

                        <div id="orange" class="animate-flower3" style="position:absolute; z-index:9; margin-left:325px; margin-top:329px;">
                            <img src="~/Content/images/flower_orange.png" style="width:140px;" />
                        </div>

                        <div id="yellow2" class="animate-flower4" style="position:absolute; z-index:8; margin-left:214px; margin-top:329px;">
                            <img src="~/Content/images/flower_yellow.png" style="width:140px;" />
                        </div>

                        <div id="red" class="animate-flower" style="position:absolute; z-index:8;">
                            <img src="~/Content/images/flower_red.png" style="width:140px;" />
                        </div>

                        <div id="yellow3" class="animate-flower5" style="position:absolute; z-index:8; margin-left:405px; margin-top:285px;">
                            <img src="~/Content/images/flower_yellow.png" style="width:140px;" />
