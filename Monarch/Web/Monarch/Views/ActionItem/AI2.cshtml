            function birdFlap() {
                if (gameState === 1 || gameState === 2) {
                    $bird.css('transform', 'rotate(-20deg)');
                    $bird.stop().animate({
                        top: '-=60px'
                    }, 200, function () {
                        birdPos();
                        $bird.css('transform', 'rotate(0deg)');
                        $bird.stop().animate({
                            top: '+=160px'
                        }, 300, 'linear', function () {
                            birdPos();
                            gravity();
                            testCollision();
                        });
                    });
                }
            }

            // TAPPING EFFECT

            function gravity() {
                birdPercent = parseInt($bird.css('bottom')) / $window.height();
                totalFallTime = fallTime * birdPercent;
                $bird.stop().animate({
                    top: '265px'
                }, totalFallTime, 'linear');
            }

