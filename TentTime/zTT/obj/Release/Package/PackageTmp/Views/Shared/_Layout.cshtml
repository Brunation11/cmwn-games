<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Change My World Now</title>
    @Styles.Render("~/bundles/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryui")
    @RenderSection("scripts", required: false)



    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/themes/smoothness/jquery-ui.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js"></script>


    <style type="text/css">
        .ai_background {
            background-color: #7829bb;
            width: 622px;
            padding: 3px;
            position:absolute;
        }

        .ai-margin {
            margin: 3px;
        }

        .custom-font {
            font-family: 'cmwn';
        }

        .game-title {
            font-size: 42px;
            color: white;
        }

        .game-text {
            font-size: 28px;
            color: white;
        }

        .bkg-image img {
            width: 616px;
        }

        .text-left {
            margin: 5px;
        }

        .button-close {
            width: 100px;
            height: 40px;
            background: #7829bb;
            text-align: center;
            margin-left: 139px;
            margin-top: 250px;
        }

        .button-close a {
            font-size: 18px;
            color: #ffffff;
        }
        .modal-backdrop{
            background-color: #7829bb;           
            opacity: .2;
            filter: alpha(opacity=20);
            width: 100%;
            height: 100%;
            position: absolute;
            margin: 0 auto;
        }
        .fade .in{
            position:absolute;
        }


        .modal {
            overflow-y:hidden;
        }

        .modal-open .modal {
        overflow-x: hidden;
        overflow-y: hidden;
        }

        .purple-button {
            width: 180px;
            height: 32px;
            margin: 0px 0 0;
            padding: 8px 3px 3px 5px;
            background-color: #7829bb;
            -webkit-border-radius: 8px;
            -moz-border-radius: 8px;
            border-radius: 8px;
        }

        .green-button {
            width: 180px;
            height: 32px;
            margin: 0px 0 0;
            padding: 8px 3px 3px 5px;
            background-color: #58CC20;
            -webkit-border-radius: 8px;
            -moz-border-radius: 8px;
            border-radius: 8px;
        }

        .button-text a{
            font-size: 20px;
            color: white;
        }
        .button-padding{
            padding: 0px 50px 0px 50px;
        }
    </style>

    <script type="text/javascript">
        var start = document.getElementById("backg");
        var clk = document.getElementById("click");
        var ca = document.getElementById("correct");
        var InC = document.getElementById("incorrect");
        var sel = document.getElementById("select");
        var su = document.getElementById("sign_up");
        var s1 = document.getElementById("badg");
        var s2 = document.getElementById("dee");
        var s3 = document.getElementById("eagl");
        var s4 = document.getElementById("forest");
        var s5 = document.getElementById("oposs");
        var s6 = document.getElementById("ow");
        var s7 = document.getElementById("rac");
        var s8 = document.getElementById("tur");
        var s9 = document.getElementById("wol");


        $(document).ready(function () {


            $('.close-Btn').click(function () {
                $('#game-info').modal('show');
            });

       

            $('#close_2').click(function () {

                $("#game-info").modal('hide');



            });



        });
    </script>

</head>
