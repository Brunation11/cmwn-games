        </div>

        <!--Place correct one here-->
        <div id="correct_btn">
            <img src="~/Content/images/border.png" class="img-responsive" alt="Responsive image" style="position: absolute; z-index: 2; width: 280px;margin-left: 295px;margin-top: 233px;" />
            <img src="~/Content/images/spider.png" class="img-responsive" alt="Responsive image" style="position: absolute; z-index: 1; width: 260px;margin-left: 305px;margin-top: 238px;" />
        </div>

        <!--close button-->
        <div id="close-Btn" style=" position: absolute;  z-index: 1;  margin-left: 563px;  margin-top: 13px;">

            <div class="flip-on" style="z-index:10;"><img src="~/Content/images/btn_exit_on.png" class="img-responsive" alt="Responsive image" style=" width: 32px; margin-bottom: -32px;"></div>
