
@{
    ViewBag.Title = "AI8";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style type="text/css">
    #tree img {
        float: left;
        margin-left: -10px;
        margin-top: -424px;
    }

    #sign img {
        float: right;
        margin-top: -160px;
        margin-right: 33px;
    }

    #close img {
        float: right;
        margin-top: -393px;
        margin-right: -118px;
    }

    .content-box {
        margin: 0px -5px 0;
    }

    #bframe img {
        align-content: center;
        margin-top: -338px;
        margin-left: 117px;
    }

    #bframe {
        z-index: 1001;
        position: inherit;
        margin-bottom: -18px;
        height: 428px;
        width: 331px;
    }

   .ai_background {
    background-color: #7829BB;
    padding-top: 10px;
    width: 622px;
    height: 451px;
    padding-bottom: 20px;
}
   

    #bframe-content {
        background-color: #fff;
        height: 195px;
        width: 262px;
        margin-top: -349px;
        z-index: 1000;
        margin-left: 154px;
        position: inherit;
        opacity: 0.9;
        -webkit-box-shadow: 6px 1px 12px 0px rgba(50, 50, 50, 0.57);
        -moz-box-shadow: 6px 1px 12px 0px rgba(50, 50, 50, 0.57);
        box-shadow: 6px 1px 12px 0px rgba(50, 50, 50, 0.57);
    }

        #bframe-content p {
            font-family: 'Source Sans Pro';
            font-size: 24px;
            line-height: 34px;
            color: #252525;
            width: 207px;
            margin-left: 15px;
            text-align: center;
            margin-top: -243px;
            margin-bottom: 227px;
        }
</style>

<div class="row">
    
    <div class="col-md-12 content-box" style="margin-left:20px;">
        <div class="col-md-12 custom-font content-title">
            <div class="row">
                @Html.Partial("_game")
            </div>
        </div>
    </div>
</div>