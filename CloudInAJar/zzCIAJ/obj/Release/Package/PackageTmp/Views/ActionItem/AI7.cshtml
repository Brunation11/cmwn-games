                <div class="flip-on" style="z-index:10;"><img src="~/Content/images/btn_yes_on.png" class="img-responsive" alt="Responsive image" style="width: 151px; margin-left: 89px; position: absolute; z-index: 8; margin-top: 300px;"></div>
                <div class="flip-off" style="z-index:9;"><img src="~/Content/images/btn_yes.png" class="img-responsive" alt="Responsive image" style=" width: 151px; position: absolute; margin-left: 89px; z-index: 9; margin-top: 300px;"></div>
            </div>

            <div id="btn1">
                <div class="flip-on" style="z-index:10;"><img src="~/Content/images/btn_no_on.png" class="img-responsive" alt="Responsive image" style="width: 151px; margin-left: 352px; position: absolute; z-index: 8; margin-top: 300px;"></div>
                <div class="flip-off" style="z-index:9;"><img src="~/Content/images/btn_no.png" class="img-responsive" alt="Responsive image" style=" width: 151px; position: absolute; margin-left: 352px; z-index: 9; margin-top: 300px;"></div>
            </div>



        </div>
