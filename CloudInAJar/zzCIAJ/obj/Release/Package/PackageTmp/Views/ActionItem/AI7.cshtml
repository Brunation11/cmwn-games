
@{
    ViewBag.Title = "AI7";
}





<style>
    #title img {
        margin-top: 66px;
        margin-left: 92px;
        margin-right: 0px;
        position: absolute;
        z-index: 1;
    }


    #sign img {
        float: right;
        margin-top: 384px;
        margin-left: 466px;
        position: absolute;
    }



    .flip-off img {
        position: absolute;
        -webkit-transition: opacity 0s ease-in-out;
        -moz-transition: opacity 0s ease-in-out;
        -o-transition: opacity 0s ease-in-out;
        transition: opacity 0s ease-in-out;
    }

        .flip-off img:hover {
            position: absolute;
            opacity: 0;
        }


    p, .p {
  font-family: Source Sans Pro;
  font-size: 23px;
  position: absolute;
  z-index: 2;
  width: 385px;
  text-align: center;
  margin-top: 171px;
  margin-left: 119px;
  line-height: 30px;
}


    h2, .h2 {
        font-family: Source Sans Pro Semibold;
        font-size: 28px;
        position: absolute;
        text-align: center;
        z-index: 2;
        margin-top: 198px;
        margin-left: 101px;
    }

    b, strong {
  font-weight: 900;
  font-family: Source Sans Pro;
  font-size: 27px;
}
</style>


<script type="text/javascript">
    $(document).ready(function () {
        load_8.play();

        $('#btn').click(function () {
            $('#btn').delay(1000).delay(1000, function () {
                document.location.href = "@Url.Content("~/ActionItem/AI17")";
            });
            try {
                btn_click.play();
            }
            catch (err) {
                //no sound - log error
            }
        });


        $('#btn1').click(function () {
            $('#btn1').delay(1000).delay(1000, function () {
                document.location.href = "@Url.Content("~/ActionItem/AI6")";
            });
            try {
                btn_click.play();
            }
            catch (err) {
                //no sound - log error
            }
        });
    })
</script>



