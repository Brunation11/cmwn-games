    <style type="text/css">
        .ai_background {
            background-color: #7829bb;
            width: 622px;
            padding: 3px;
            position:absolute;
        }

         .close-Btn {
        margin-top: 10px;
        position: absolute;
        z-index: 1;
        left: 580px;
    }


        .ai-margin {
            margin: 3px;
        }

        .custom-font {
            font-family: 'cmwn';
        }

        .game-title {
            font-size: 42px;
            color: white;
        }

        .game-text {
            font-size: 28px;
            color: white;
        }

        .bkg-image img {
            width: 616px;
        }

        .text-left {
            margin: 5px;
        }

        .button-close {
            width: 100px;
            height: 40px;
            background: #7829bb;
            text-align: center;
            margin-left: 139px;
            margin-top: 250px;
        }

        .button-close a {
            font-size: 18px;
            color: #ffffff;
        }
        .modal-backdrop{
            background-color: #7829bb;           
            opacity: .2;
            filter: alpha(opacity=20);
            width: 100%;
            height: 100%;
            position: absolute;
            margin: 0 auto;
        }
        .fade .in{
            position:absolute;
        }

        .modal-open .modal {
  overflow-x: hidden;
  overflow-y: hidden;
}


        .purple-button {
            width: 180px;
            height: 32px;
            margin: 0px 0 0;
            padding: 8px 3px 3px 5px;
            background-color: #7829bb;
            -webkit-border-radius: 8px;
            -moz-border-radius: 8px;
            border-radius: 8px;
        }

        .green-button {
            width: 180px;
            height: 32px;
            margin: 0px 0 0;
            padding: 8px 3px 3px 5px;
            background-color: #58CC20;
            -webkit-border-radius: 8px;
            -moz-border-radius: 8px;
            border-radius: 8px;
        }

        .button-text a{
            font-size: 20px;
            color: white;
        }
        .button-padding{
            padding: 0px 50px 0px 50px;
        }

        .flip-off img {
        position: absolute;
        -webkit-transition: opacity 0s ease-in-out;
        -moz-transition: opacity 0s ease-in-out;
        -o-transition: opacity 0s ease-in-out;
        transition: opacity 0s ease-in-out;
    }

        .flip-off img:hover {
            position: absolute;
            opacity: 0;
        }

         .close-Btn {
        margin-top: 10px;
        position: absolute;
        z-index: 1;
        left: 580px;
    }
    </style>

