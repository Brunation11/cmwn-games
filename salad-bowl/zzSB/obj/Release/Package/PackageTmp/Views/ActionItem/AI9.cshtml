
@{
    ViewBag.Title = "AI9";
}



<script type="text/javascript">
    $(document).ready(function () {
        try {
            flip.load();
            flip.play();
        }
        catch (err) {
            //no sound - log error
        }

        $('.play img').hide();
        $('.play img').fadeIn(4000);

        $('.play').click(function () {
            $('.play').delay(1000).delay(1000, function () {
                document.location.href = "@Url.Content("~/ActionItem/AI1")";
            });
            try {
                click.play();
            }
            catch (err) {
                //no sound - log error
            }
        });
    });
</script>



