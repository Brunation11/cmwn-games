
@{
    ViewBag.Title = "AI1";
}


<style type="text/css">
    .play {
    position:absolute;
    }
</style>

<script type="text/javascript">
    $(document).ready(function () {
        try
        {
            title.load();
            title.play();
        }
        catch(err)
        {
            //no sound - log error
        }
        

    $('.play').click(function () {
        $('.play').delay(1000).delay(1000, function () {
            document.location.href = "@Url.Content("~/ActionItem/AI2")";
        });
        try
        {
            click.play();
        }
        catch(err)
        {
            //no sound - log error
        }
        
    });
    });
</script>







<div class="container body-content">
    <div class="col-md-12">
        <div class="row">


            <div class="close-Btn">
                <div class="flip-on" style="z-index:10;"><img src="~/Content/images/btn_exit_on.png" style="width:50px;" /></div>
                <div class="flip-off" style="z-index:9;"><img src="~/Content/images/btn_exit.png" style="width:50px;margin-top: -52px;" /></div>
                </div>


            <img src="~/Content/images/BKG_1.png" style="width:616px;position: absolute;z-index: 1;" />
            <img src="~/Content/images/title_2.png" style="width:616px;position: absolute;z-index: 2;" />

           
            <div class="play">
             <div class="flip-on" style="z-index:10;"><img src="~/Content/images/btn_play_on.png" style="width: 139px;position:absolute;z-index: 2;margin-top: 292px;margin-left: 471px;" /></div>
            <div class="flip-off" style="z-index:9;"><img src="~/Content/images/btn_play.png" style="width: 139px;margin-top: 292px;position:absolute;z-index: 2;margin-left: 471px;" /></div>
            </div>
        </div>
    </div>
</div><!--end container body-content-->

