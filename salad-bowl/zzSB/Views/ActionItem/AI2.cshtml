
@{
    ViewBag.Title = "AI2";
}







<script type="text/javascript">
    $(document).ready(function () {
        $('.play img').hide();
        $('.play img').fadeIn(4000);

        $('.play').click(function () {
            $('.play').delay(1000).delay(1000, function () {
                document.location.href = "@Url.Content("~/ActionItem/AI3")";
            });
         
            try {
                click.play();
            }
            catch (err) {
                //no sound - log error
            }
        });
    });
</script>



<div class="container body-content">
    <div class="col-md-12">
        <div class="row">


            <div class="close-Btn">
                <div class="flip-on" style="z-index:10;"><img src="~/Content/images/btn_exit_on.png" style="width:50px;" /></div>
                <div class="flip-off" style="z-index:9;"><img src="~/Content/images/btn_exit.png" style="width:50px;margin-top: -52px;" /></div>
            </div>


            <img src="~/Content/images/header_1.png" style="width:616px;position: absolute;z-index: 1;" />
            <img src="~/Content/images/salad_1.png" style="width:616px;position: absolute;z-index: 2;margin-top: 48px;margin-left: -10px ;" />


            <div class="play">
                <div class="flip-on" style="z-index:10;"><img src="~/Content/images/btn_next_on.png" style="width: 89px;position:absolute;z-index: 2;margin-top: 319px;margin-left: 524px;" /></div>
                <div class="flip-off" style="z-index:9;"><img src="~/Content/images/btn_next.png" style="width: 89px;margin-top: 319px;position:absolute;z-index: 2;margin-left: 524px;" /></div>
            </div>
        </div>
    </div>
</div><!--end container body-content-->
